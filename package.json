{
  "name": "vite-react-ts-bun-starter",
  "author": "bsc7th",
  "description": "A minimal Vite + React + TypeScript starter using Bun, with Prettier and import sorting built in.",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "tsc -b && bunx --bun vite build",
    "format": "bunx --bun prettier --write .",
    "preview": "vite preview",
    "lint": "bunx --bun eslint ."
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/bun": "^1.2.13",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.26.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3"
  }
}
